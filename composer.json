{
  "name": "jacobemerick/jakemvc",
  "description": "A simple, custom MVC that powers several of my personal sites, including a blog, lifestream, and waterfall directory.",
  "keywords": [
    "mvc",
    "framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jacob Emerick"
    }
  ],
  "require": {
    "php": ">=5.4",
    "aura/sql": "^2.4",
    "jacobemerick/monolog-pqp": "^0.1",
    "madcoda/php-youtube-api": "^1.2",
    "monolog/monolog": "^1.17",
    "particletree/pqp": "^1.1",
    "pimple/pimple": "^3.0",
    "suin/php-rss-writer": "^1.3",
    "thepixeldeveloper/sitemap": "^4.0",
    "jacobemerick/archangel": "^2.1"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "classmap": [
      "utility/"
    ],
    "psr-4": {
      "Jacobemerick\\Web\\": "src/"
    }
  }
}
