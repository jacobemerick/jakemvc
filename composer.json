{
  "name": "jacobemerick/jakemvc",
  "description": "A simple, custom MVC that powers several of my personal sites, including a blog, lifestream, and waterfall directory.",
  "keywords": [
    "mvc",
    "framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jacob Emerick"
    }
  ],
  "require": {
    "php": ">=5.4",
    "aura/sql": "^2.4",
    "particletree/pqp": "^1.0",
    "pimple/pimple": "^3.0",
    "thepixeldeveloper/sitemap": "^4.0",
    "monolog/monolog": "^1.17",
    "suin/php-rss-writer": "^1.3",
    "knplabs/github-api": "^1.5"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "classmap": [
      "collector/",
      "controller/",
      "module/",
      "router/",
      "utility/"
    ],
    "psr-4": {
      "Jacobemerick\\Web\\": "src/"
    }
  }
}
